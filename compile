# for building KDE4 frontend (LabPlot) just use 

if [ -d build ]; then
	echo "directory build/ exists! Please remove it."
else
	mkdir build
fi

cd build && cmake .. -DCMAKE_INSTALL_PREFIX=/usr && make -j 2

